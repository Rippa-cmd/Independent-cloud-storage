            </font>
        </Label>
        <TextField fx:id="fileNameField" layoutX="33.0" layoutY="75.0" prefHeight="25.0" prefWidth="250.0"/>
        <Button layoutX="231.0" layoutY="111.0" mnemonicParsing="false" onAction="#rename" text="Ok"/>
    </children>
