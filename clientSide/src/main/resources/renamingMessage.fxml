            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label alignment="CENTER" layoutX="7.0" layoutY="14.0" prefHeight="47.0" prefWidth="300.0"
               text="Enter the new name for file" textAlignment="CENTER" wrapText="true">
